{
  "expo": {
    "name": "AmbridgeMobile",
    "slug": "AmbridgeMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ambridgelogo1.png",
    "scheme": "ambridgemobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.shankerpillai.AmbridgeMobile"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ambridgelogo1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "99dbd72e-8a37-4994-873d-a88bc2819e72"
      }
    }
  }
}
